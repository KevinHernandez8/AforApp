<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aforapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_app_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".VelocidadActivity"
            android:label="@string/velocidad" />
        <activity
            android:name=".OcupacionTaxiActivity"
            android:label="@string/ocupacionTaxi" />
        <activity
            android:name=".OcupacionTPCActivity"
            android:label="@string/ocupacionTPC" />
        <activity
            android:name=".VolumenMotorizadoActivity"
            android:label="@string/volumenMotorizado" />
        <activity
            android:name=".VolumenNoMotorizadoActivity"
            android:label="@string/volumenNoMotorizado" />
        <activity
            android:name=".CategoriaActivity"
            android:label="@string/categoria" />
        <activity
            android:name=".InfoActivity"
            android:label="@string/info_title" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>